{
  "name": "floxy-manager",
  "version": "1.0.0",
  "description": "Web Manager for floxy",
  "scripts": {
    "tw:dev": "tailwindcss -i ./src/tailwind.css -o ./src/tw.css --watch",
    "tw:build": "tailwindcss -i ./src/tailwind.css -o ./src/tw.css --minify",
    "build": "npm run tw:build && webpack --mode production",
    "dev": "npm run tw:dev & webpack serve --mode development",
    "install-deps": "npm install"
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "style-loader": "^3.3.3",
    "tailwindcss": "^4.1.16",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.16"
  }
}
